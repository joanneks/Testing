{{#extends 'base'}}
{{#block 'content'}}
    <form method="POST">
        <h1>Delete food sighting</h1>
            <div class="alert alert-danger">
                Are you sure you want to delete this food record?
                "{{foodSighting.description}}" added at "{{foodSighting.datetime}}""
            </div>
            <input type="submit" class="btn btn-danger"/>
            <a href="/" class="btn btn-success">Cancel</a>
    </form>
    
{{/block}}
{{/extends}}